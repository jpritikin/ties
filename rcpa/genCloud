### BEGIN GLOBAL STATE

our $dfh;
open $dfh, ">:encoding(utf8)", "demogr.csv";

our $RCPA_DEMOGRAPHICS;
our $PA1_START;
our $PA2_START;
our $RCPA_START;
our $PAID_START;
our @EndNote;

my %VettedName;
my %NodeInfo;
my %NodeGroupInfo;
my %EdgeInfo;
my $Graph = Graph::Undirected->new();

for my $k (values %LongDict) {
    if (!exists $ShortDict{$k}) {
	$ShortDict{$k} = $k
    }
}
for my $k (values %ShortDict, values %LongDict) {
    die "Activity mapped to the empty string" if $k eq '';
    $VettedName{$k} = 1
}

### END GLOBAL STATE

sub trim {
    my ($str) = @_;
    $str =~ s/^\s+//;
    $str =~ s/\s+$//;
    $str
}

sub useNode {
    my ($pa,$solo) = @_;
    return if $pa eq 'invalid';
    if (exists $GroupSoloOverride{$pa}) {
	$solo = $GroupSoloOverride{$pa};
    }
    my $name = join(';', $pa, lc $solo);
    if (!exists $NodeInfo{$name}) {
	$NodeInfo{$name} = { count => 0 };
    }
    ++$NodeInfo{$name}{count};
    ++$NodeGroupInfo{$pa}{$solo};
    $name
}

sub node {
    my ($short, $long, $solo) = @_;
    $long = lc trim($long);
    $long =~ s/\"//g;
    $long =~ s/[\r\n]+/ /g;
    $short = lc trim($short);
    $short =~ s/\"//g;
    $short =~ s/\@//g;
    if ($long eq '' or $short eq $long) {
	my $ok= exists $ShortDict{$short};
	while (exists $ShortDict{$short} and $short ne $ShortDict{$short}) {
	    $short = $ShortDict{$short};
	    $ok=1;
	}
	if ($ok) {
	    return useNode($short,$solo);
	} else {
	    print("   '$short' => '',   # short\n");
	}
    } else {
	$long =~ s/\'//g;
	my $full = $short . '@' . $long;
	my $ok=0;
	if (exists $LongDict{$full}) {
	    $short = $LongDict{$full};
	    $ok=1;
	}
	while (exists $ShortDict{$short} and $short ne $ShortDict{$short}) {
	    $short = $ShortDict{$short};
	}
	if ($ok) {
	    return useNode($short,$solo);
	} elsif (exists $VettedName{$short}) {
	    print "   '$full' => '$short',\n";
	} else {
	    print("   '$full' => '',\n");
	}
    }
    undef
}

